{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-idx",
      {
        "importName": "./idx"
      }
    ],
    "transform-class-properties",
    ["import", { "libraryName": "antd", "style": "true" }]
  ]
}
